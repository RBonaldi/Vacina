<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adm.vacina_digital">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cadastro"
            android:label="Cadastro"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".TelaInicialPaciente"
            android:label="TelaInicialPaciente"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".TelaInicialMedico"
            android:label="TelaInicialMedico"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".CadastroVacina"
            android:label="CadastroVacina"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".CadastroMarcacao"
            android:label="CadastroMarcacao"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>